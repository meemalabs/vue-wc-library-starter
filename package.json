{
  "name": "web-components-library-starter",
  "version": "0.0.1",
  "description": "The easiest way to ensure your media/data is always displayed quickly, securely and at the highest quality possible.",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "vitebook:dev": "vitebook dev",
    "vitebook:build": "vitebook build",
    "vitebook:preview": "vitebook preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier . --write",
    "fix-code-style": "lint && format",
    "test": "vitest",
    "test:ci": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "vue": "^3.2.32"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.3",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.2",
    "@vitebook/client": "^0.23.5",
    "@vitebook/core": "^0.23.5",
    "@vitebook/markdown-vue": "^0.23.5",
    "@vitebook/theme-default": "^0.23.5",
    "@vitebook/vue": "^0.23.5",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/compiler-sfc": "^3.2.33",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.3.1",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-vue": "^8.6.0",
    "postcss": "^8.3.6",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "svelte": "^3.47.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-vue-components": "^0.19.3",
    "vite": "^2.9.5",
    "vite-plugin-eslint": "^1.4.0",
    "vitest": "^0.9.3",
    "vue-tsc": "^0.34.7"
  },
  "engines": {
    "npm": ">=6.0.0",
    "node": ">=14.*"
  },
  "contributors": [
    "Chris Breuer <chris@meema.xyz>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/meemalabs/web-components-library-starter.git"
  },
  "bugs": {
    "url": "https://github.com/meemalabs/web-components-library-starter/issues"
  },
  "keywords": [
    "web-components",
    "component",
    "library",
    "vue",
    "typescript",
    "javascript"
  ]
}
